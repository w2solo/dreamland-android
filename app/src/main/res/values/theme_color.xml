<resources>
    <color name="theme_color_primary">#FFFFFFFF</color>
    <color name="theme_color_secondary">#FF529af8</color>
    <color name="theme_text_color_secondary">#FFB8B7B7</color>
    <color name="theme_background_color_white">#FFFFFFFF</color>

    <color name="color_background">#f1f1f1</color>
    <color name="color_surface">#ffffff</color>
    <color name="color_surface_gray">#f7f7f7</color>
